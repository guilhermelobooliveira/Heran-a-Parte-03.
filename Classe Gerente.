package exercício2;

/**
 *
 * @author Marino
 */
public class Gerente extends Empregado{
    private String nomeGerencia;

    public String getNomeGerencia() {
        return nomeGerencia;
    }

    public void setNomeGerencia(String nomeGerencia) {
        this.nomeGerencia = nomeGerencia;
    }

    @Override
    public String toString() {
        return " O Nome do Gerente é: " + super.getNome() + " A Idade do Gerente é: " + super.getIdade() + " Matrícula Nome da Gerência: " + this.getNomeGerencia() + " Valor do INSS do Gerente é: " + super.getvalorInss();
    }
    
}
